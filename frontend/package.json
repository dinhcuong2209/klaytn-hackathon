    "lodash": "^4.17.21",
    "nuxt": "^2.15.3",
    "nuxt-i18n": "^6.27.1",
    "vue-i18n": "^8.24.4",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/tailwindcss": "^4.1.3",
